            <string>control_up=ord('W')
control_down=ord('S')
control_left=ord('A')
control_right=ord('D')

if place_empty(x,y) {
    if not place_empty(x,y+1) { //on solid ground
        if keyboard_check(control_up)  {
            y_accel=-30 //jump force         
        } else {
            y_accel=0
        }
        x_speed=0
    } else {
        y_accel=2 //gravity
    }
    if keyboard_check(control_left){
        if x_speed &lt; 0
            x_accel=-2
        else
            x_accel=-15
        if keyboard_check(control_right)
            x_accel=0 //multiple directions cancel
    }
    else {
        if keyboard_check(control_right) {
            if x_speed &gt; 0 //accelerate slow
                x_accel=2
            else 
                x_accel=15
        }
        else {//friction
            if x_speed&lt;0 
                if x_speed&lt;-6
                    x_accel=6
                else
                    x_accel=-x_speed
            else {
                if x_speed&gt;0
                    if x_speed&gt;6
                        x_accel=-6
                    else
                        x_accel=-x_speed
                else 
                    x_accel=0
            }
        }
    }
    //accelerations set, update speed
    y_speed+=y_accel
    if (x_accel&lt;0 and x_speed&gt;-15) or (x_accel&gt;0 and x_speed&lt;15) //stop accelerating when too fast
        x_speed+=x_accel
    else {
        if x_speed&lt;=-15 //cap speed
            x_speed=-15
        else if x_speed&gt;=15
            x_speed=15
    }
    //move object
    if x_speed&lt;0 {   
        move_contact_solid(180,-x_speed)
        if not place_empty(x-1,y)
            x_speed=0
    } else {
        if x_speed&gt;0 {
            move_contact_solid(0,x_speed)
            if not place_empty(x+1,y)
                x_speed=0
        }
    }
    if y_speed&lt;0 {
        move_contact_solid(90,-y_speed)
        if not place_empty(x,y-1)
            y_speed=0
    }
    else {
        if y_speed&gt;0 {
            move_contact_solid(270,y_speed)
            if not place_empty(x,y+1) { //land on solid ground
                y_speed=0
                x_speed=0
            }    
        }
    }
}
else { //inside object?
   x=200
   y=400
}
</string>
